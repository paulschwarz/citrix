{
  "name": "slakbal/citrix",
  "description": "Laravel package for Citrix/LogMeIn API wrapper for GoToWebinar",
  "keywords": [
    "citrix",
    "gotowebinar",
    "logmein",
    "laravel-gotowebinar",
    "slakbal"
  ],
  "authors": [
    {
      "name": "Leslie Price",
      "email": "leslie.price78@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "nategood/httpful": "*",
    "guzzlehttp/guzzle": "^6.1",
    "illuminate/support": "~5"
  },
  "require-dev": {
    "orchestra/testbench": "*",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Slakbal\\Citrix\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slakbal\\Citrix\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Slakbal\\Citrix\\CitrixServiceProvider"
      ],
      "aliases": {
        "Flash": "Slakbal\\Citrix\\Facade\\GotoWebinar"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "stable"
}
